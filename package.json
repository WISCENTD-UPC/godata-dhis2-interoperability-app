      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "icons": {
    "16": "favicon-16x16.png",
    "32": "favicon-32x32.png",
    "48": "favicon-48x48.png"
  },
  "manifest.webapp": {
    "name": "Go.Data-DHIS2 Interoperability App",
